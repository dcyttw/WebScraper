<br>
<div class="container-fluid article-container">
	{{#each articles}}
	<div class="panel panel-default" id={{this._id}}>
		<div class="panel-heading">
			<h3>
				<a class="article-link" target="_blank" href="{{this.link}}">
					{{this.title}}
				</a>
				<a class="btn btn-success save">Save Article</a>
			</h3>
		</div>
	</div>
	{{/each}}
</div>
